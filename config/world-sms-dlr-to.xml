<?xml version="1.0"?>
<soap:Envelope xmlns:soap="http://www.w3.org/2003/05/soap-envelope/" soap:encodingStyle="http://www.w3.org/2003/05/soap-encoding">
  <soap:Body>
    <sms:DeliveryReport xmlns:sms="https://www.w3schools.com/prices">
      <sms:smsid>%QUERY[smsid]%</sms:smsid>
      <sms:receivedts>%QUERY[receivedts]%</sms:receivedts>
      <sms:deliveredts>%QUERY[deliveredts]%</sms:deliveredts>
